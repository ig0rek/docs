{
    "move": "#int#", // номер текущего хода
    "data": {
        "stage": "#string#", // стадия игры
        "players": {
            "first": "#object#", // первый игрок
            "second": "#object#", // второй игрок
            "third": "#object#" // третий игрок
        },
        "board": {
            "deck": [], // открытые карты на столе
            "cards_in_deck": "#int#", // количество карт на столе
            "trick": "#object#", // взятка на столе
            "offer": { // доступное количество взяток для предложений
                "tricks": {
                    "min": "#int#",
                    "max": "#int#"
                }
            }
        },
        "score_sheet": { // пулька
            "bid_end": "#int#", // размер игры
            "areas": {
                "first": "#object#", // зона первого игрока
                "second": "#object#", // зона второго игрока
                "third": "#object#" // зона третьего игрока
            }
        },
        "move": {
            "prev": "#null|object#", // предыдущий ход (при старте - null)
            "next": { // следующий ход
                "player_id": "#int#", // id пользователя, кто должен ходить
                "playing_player_id": "#int#" // id пользователя, который играет (различается во время игры, когда один игрок может управлять другим)
            }
        },
        "is_end_of_round": "#bool#", // флаг о завершении раунда (торговля -> раздача карт)
        "is_en_of_match": "#bool#" // флаг о завершении всей игры
    }
}