{
    "move": 1, // номер текущего хода
    "data": {
        "board": { // "игровая доска"
            "trump": "#string#", // карта-козырь, которая лежит под колодой карт
            "thrown_cards": [], // пары карт "на столе"
            "cards_in_deck": "#int#" // количество оставшихся карт в колоде (включая последнюю открытую карту-козырь)
        },
        "players": [ // список игроков
            {
                "id": "#int#", // идентификатор игрока
                "place": "#int#", // номер места за столом, начиная с нуля
                "is_defender": "#bool#", // является ли игрок "защищающимся"/"отбивающимся"
                "status": "#null|string#", // какой тип хода был сделан игроком (по умолчанию null)
                "cards_in_hand": "#int#" // количество карт на руке
            },
            {
                "id": "#int#",
                "place": "#int#",
                "is_defender": "#bool#",
                "status": "#null|string#",
                "cards_in_hand": "#int#"
            }
        ],
        "smaller_trump": "#null|string#", // "наименьший козырь" в руке у первого ходящего игрока (если ни у кого нет козырей - null)
        "next_move": {
            "player_id": "#int#" // id пользователя, кто сейчас должен ходить
        }
    }
}